<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane prefHeight="490.0" prefWidth="717.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="oms.fx.PersonalInformationController">
    <children>
        <Label layoutX="14.0" layoutY="58.0" text="The inputs marked by * are all required" textFill="#a49f9f"/>
        <Label layoutX="15.0" layoutY="96.0" text="Child Name*"/>
        <TextField fx:id="childName" layoutX="15.0" layoutY="121.0" promptText="Child Name"/>
        <Label layoutX="209.0" layoutY="96.0" text="Father Name*"/>
        <TextField fx:id="fatherName" layoutX="209.0" layoutY="121.0" promptText="Father Name"/>
        <Label layoutX="398.0" layoutY="96.0" text="Grandfather Name*"/>
        <TextField fx:id="grandfatherName" layoutX="398.0" layoutY="121.0" promptText="Grandfather Name"/>
        <Label layoutX="209.0" layoutY="159.0" text="Date of Birth*"/>
        <TextField fx:id="childDateOfBirth" layoutX="209.0" layoutY="182.0" promptText="Date of Birth"/>
        <Label layoutX="398.0" layoutY="159.0" text="Place of Birth*"/>
        <TextField fx:id="childPlaceOfBirth" layoutX="398.0" layoutY="182.0" promptText="Place of Birth"/>
        <Label layoutX="17.0" layoutY="243.0" text="Spoken Language(s)"/>
        <TextField fx:id="spokenLanguages" layoutX="17.0" layoutY="269.0" promptText="Add Languages"/>
        <Button layoutX="639.0" layoutY="440.0" mnemonicParsing="false" onAction="#next" text="Save"
                textFill="#1a00ff"/>
        <Label layoutX="320.0" layoutY="20.0" text="Personal Information">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Label layoutX="15.0" layoutY="159.0" prefHeight="17.0" prefWidth="75.0" text="Gender*"/>
        <RadioButton fx:id="Male" layoutX="15.0" layoutY="182.0" mnemonicParsing="false" onAction="#male" text="Male"/>
        <RadioButton fx:id="Female" layoutX="15.0" layoutY="207.0" mnemonicParsing="false" onAction="#female"
                     text="Female"/>
    </children>
</Pane>
